/**
 * @author Alegrarsio gifta lesmana
 * Csv class provides the sets of function to load CSV file 
**/
class Csv{
    
    /**
     * init()
     * @param filename you can also use the path such as /bin/data/data.csv
     * @return None
    **/
    init(filename : String) -> None = this.filename = filename
    
    /**
     * load() is a function to load csv file 
     * @param None
     * @return Array
    **/
    func load() -> Array = __read_csv(this.filename)

}