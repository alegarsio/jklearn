class PreProcessor {

    init(data : Array){
        this.data = data;
    }

    func split(ratio) {
        return __split(this.data, ratio)
    }

    func standardize() {
        this.data = native_standardnize(this.data)
        return this
    }


    func smooth(period) {
        this.data = native_smooth(this.data, period)
        return this
    }

    func exports(){
        return this.data 
    }

}